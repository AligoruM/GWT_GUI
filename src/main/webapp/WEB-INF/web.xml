<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>com.mySampleApplication.MySampleApplication DownloadLibService</servlet-name>
        <servlet-class>com.MyLibraryWebApplication.server.services.DownloadLibServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.MyLibraryWebApplication.MyLibraryWebApplication UploadLibService</servlet-name>
        <servlet-class>com.MyLibraryWebApplication.server.services.UploadLibServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>com.MyLibraryWebApplication.MyLibraryWebApplication SortByService</servlet-name>
        <servlet-class>com.MyLibraryWebApplication.server.services.SortByServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.mySampleApplication.MySampleApplication DownloadLibService</servlet-name>
        <url-pattern>/MyLibraryWebApplication/DownloadLib</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.MyLibraryWebApplication.MyLibraryWebApplication UploadLibService</servlet-name>
        <url-pattern>/MyLibraryWebApplication/UploadLibService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.MyLibraryWebApplication.MyLibraryWebApplication SortByService</servlet-name>
        <url-pattern>/MyLibraryWebApplication/SortByService</url-pattern>
    </servlet-mapping>
</web-app>